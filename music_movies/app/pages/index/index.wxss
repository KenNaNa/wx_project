page {
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  background: #f1f1f1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.img {
  width: 392rpx;
  height: 96rpx;
  margin-top: 160rpx;
}

.username {
  font-size: 50rpx;
  font-family: "微软雅黑";
  font-weight: bold;
  margin-top: 0rpx;
  color: #109d59;
  text-shadow: 0px 0px 5px #109d59;
}

.btn {
  width: auto;
  height: 20px;
  padding: 10px;
  border-radius: 5px;
  line-height: 15px;
  box-shadow: inset 0px -5px 5px #109d59;
  border: 1px solid #109d59;
  color: #109d59;
  text-align: center;
  margin-top: 50rpx;
  text-shadow: 0px 0px 5px #109d59;
}
